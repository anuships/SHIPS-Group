<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ships.myapplication">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/HiddenTitleTheme">
        <activity
            android:name=".loginregistration.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/HiddenTitleTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".programlist.FutureProgram"
            android:label="Future Program"
            android:exported="true"/>
        <activity
            android:name=".EMDR.EMDRActivity"
            android:exported="true"
            android:theme="@style/HiddenTitleTheme" />
        <activity
        android:name=".loginregistration.TermsAndConditions"
        android:exported="true" />
        <activity
            android:name=".Exposure.ExposureInfo"
            android:exported="true" />
        <activity
            android:name=".loginregistration.Register"
            android:exported="true" />
        <activity android:name=".loginregistration.RegisterSuccess"
            android:exported="true"/>
        <activity android:name=".programlist.ModuleSelection"
            android:exported="true"/>
        <activity android:name=".programlist.ModuleSuggestion"
            android:exported="true"/>
        <activity
            android:name=".GSR.GSRGraphActivity"
            android:exported="true" />
        <activity
            android:name=".FAS.FAS"
            android:exported="true" />
        <activity
            android:name=".relaxaudio.RelaxationAudioActivity"
            android:exported="true"></activity>
        <activity android:name=".FAS.ShowFASResult"></activity>
        <activity
            android:name=".loginregistration.LoginActivity"
            android:exported="true" />
    </application>

</manifest>